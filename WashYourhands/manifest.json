{
    "description": "A reminder to wash your hand every 60 mins in this covid-19 crisis.",
    "homepage_url": "https://github.com/nabendu82/Addons",
    "icons": {
        "64": "icons/icon.png"
    },
    "version": "1.0",
    "manifest_version": 2,
    "name": "Wash Your Hands",
    "permissions": [
        "alarms",
        "notifications"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_title": "Wash Your Hands"
    },
    "applications": {
        "gecko": {
            "strict_min_version": "55.0a2"
        }
    }
}